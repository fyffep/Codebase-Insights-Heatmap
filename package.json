{
  "name": "codebase-insights",
  "displayName": "Codebase Insights",
  "description": "Codebase Insights will help you understand when a commit will break a build. Additionally, it will help guide your refactoring by evaluating and presenting possible troublesome files within the codebase.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:codebaseInsightsTree"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codebase-insights.overview",
        "title": "Open Codebase Insights Overview page",
        "category": "Codebase Insight Tools"
      },
      {
        "command": "codebase-insights.code-map",
        "title": "Open Codebase Insights Code Map page",
        "category": "Codebase Insight Tools"
      },
      {
        "command": "codebase-insights.knowledge-graph",
        "title": "Open Codebase Insights Knowledge Graph page",
        "category": "Codebase Insight Tools"
      },
      {
        "command": "codebase-insights.commit-risk-assessment",
        "title": "Open Codebase Insights Commit Risk Assessment page",
        "category": "Codebase Insight Tools"
      },
      {
        "command": "codebase-insights.insights",
        "title": "Open Codebase Insights Insights page",
        "category": "Codebase Insight Tools"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codebase-insights",
          "title": "Codebase Insights",
          "icon": "resources/owl.svg"
        }
      ]
    },
    "views": {
      "codebase-insights": [
        {
          "id": "codebaseInsightsTree",
          "name": "Navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "d3": "^7.3.0"
  }
}
